<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
		"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="gmail.contract.dao.LocationMapper">
	<select id="getAllLocations" resultType="Location">
		SELECT l.*
		FROM
			`location` l
		WHERE 1;
	</select>
</mapper>